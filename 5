#include<stdio.h>   
#define N 100   
int main()   
{   
    int i,j,count,n,s,b,c;   
    int a[N][N];   
    scanf("%d",&n);   
    for(i=0;i<n;i++)   
        for(j=0;j<n;j++)   
        scanf("%d",&a[i][j]);   
        count=0;   
        for(i=0;i<n;i++)   
        {   
            s=0;   
         for(j=0;j<n;j++)   
         {   
             s+=a[i][j];   
         }   
         if(s%2!=0)   
         {   
             count++;   
             b=i;   
         }   
        }   
        for(j=0;j<n;j++)   
        {   
            s=0;   
         for(i=0;i<n;i++)   
         {   
             s+=a[i][j];   
         }   
         if(s%2!=0)   
         { count++;   
           c=j;   
         }   
        }   
        if(count==0)   
            printf("OK");   
        else if(count==2&&c>=0&&b>=0)   
            printf("Change bit(%d,%d)",b,c);   
        else  
        printf("Corrupt");   
        printf("\n");   
            return 0;   
}   
