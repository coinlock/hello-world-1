#include<iostream>
using namespace std;

int main()
{
	int n;
	cin>>n;
	if(n>0&&n<8)
	{
		if(n==1)
		{
		cout<<2;
	}
		if(n==2)
		{
		cout<<5;
	}
		if(n==3)
		{
		cout<<1;
	}
		if(n==4)
	{
		
			cout<<7;
	}
		if(n==5)
	{
		
			cout<<2;
	}
		if(n==6)
	{
	
			cout<<3;
	}
		if(n==7)
	{
			cout<<0;												
	}
	}else{
		cout<<"Input Error";
	}
}
