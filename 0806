#include<stdio.h>

int main()
{
	int x,y,w,h;
	int i,ii;
	char A[7][11]={"0","0__**_**__","0_*__*__*_","0_*_____*_","0__*___*__","0___*_*___","0____*____"};
    scanf("%d %d %d %d",&x,&y,&w,&h);


    
  if((x>=1)&&(x<=9)&&(y>=1)&&(y<=6)&&(w>=1)&&(w<=9)&&(h>=1)&&(h<=6)&&((x+w)>=2)&&((x+w)<=10)&&((y+h)>=2)&&((y+h)<=7))
 {
	
	for(ii=y;ii<y+h;ii++)
	{
	
    for(i=x;i<w+x;i++)
    {
    	printf("%c",A[ii][i]);
    
	}
	printf("\n");
    }
}else{	printf("Input Error");}
	 return 0;
	 
	 
 } 
